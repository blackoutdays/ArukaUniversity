@model arusha.Models.ProgramUni

<h2>Program Details</h2>

<div>
    <h4>Program Name: @Model.Name</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            University ID
        </dt>
        <dd class="col-sm-10">
            @Model.UniversityId
        </dd>
        <dt class="col-sm-2">
            Academic Department ID
        </dt>
        <dd class="col-sm-10">
            @Model.AcademicDepartmentId
        </dd>
        <dt class="col-sm-2">
            Start Date
        </dt>
        <dd class="col-sm-10">
            @Model.StartDate
        </dd>
        <dt class="col-sm-2">
            End Date
        </dt>
        <dd class="col-sm-10">
            @Model.EndDate
        </dd>
        <dt class="col-sm-2">
            Description
        </dt>
        <dd class="col-sm-10">
            @Model.Description
        </dd>
    </dl>
</div>
